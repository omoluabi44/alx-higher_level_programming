#!/usr/bin/python3
""" POST method """

import sys
import urllib.request
if __name__ == "__main__":
    url = sys.argv[1]
    try:
        with urllib.request.urlopen(url) as response:
            response_body = response.read().decode('utf-8')
            urllib.error.H
        print(response_body)
    except urllib.error.HTTPError:
       print(f"error:{response_code}")
