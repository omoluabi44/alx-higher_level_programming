--  lists the number of records with the same score in the table second_table of the database hbtn_0c_0 in your MySQL server.


SELECT score,  COUNT(*) as number
FROM second_table
GROUP BY score
ORDER BY SCORE DESC;
--  lists the number of records with the same score in the table second_table of the database hbtn_0c_0 in your MySQL server.


SELECT score, name
FROM second_table
WHERE name IS NOT NULL
ORDER BY SCORE DESC;
